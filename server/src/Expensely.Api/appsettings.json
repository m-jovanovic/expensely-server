{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.RavenDB" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Quartz": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*",
  "RavenDB": {
    "Certificate": "",
    "Database": "",
    "Urls": ""
  },
  "Jwt": {
    "Issuer": "",
    "Audience": "",
    "SecurityKey": "",
    "TokenExpirationInMinutes": 0,
    "RefreshTokenExpirationInMinutes": 0
  }
}

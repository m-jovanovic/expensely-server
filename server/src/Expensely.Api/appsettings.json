{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*",
  "RavenDB": {
    "CertificatePath": "",
    "Database": "",
    "Urls": ""
  },
  "Jwt": {
    "Issuer": "",
    "Audience": "",
    "SecurityKey": "",
    "TokenExpirationInMinutes": 0,
    "RefreshTokenExpirationInMinutes": 0
  }
}
